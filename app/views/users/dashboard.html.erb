 <%= current_user.quizzes.each do |q| %>
            <div id='container' class='accordion'>
                <ol>
                    <li>
                        <h2><span>Pending</span></h2>
                        <div id="pending" class="quizzes">
                          <div class="quiz well">
                            <h6 class="for-dash"><%= link_to q.name, quiz_path(q) %></h6>
                            <%= link_to 'Delete', q, method: :delete, class: "span1.5 quiz-link btn btn-primary", data: { confirm: 'Are you sure?' } %>
                            <a class="span1.5 quiz-link btn btn-primary" href="<%=review_quiz_questions_path(q)%>">View Submitted Questions</a>
                          </div>
                        </div>
                    </li>
                    <li>
                        <h2><span>Active</span></h2>
                        <div id = 'active'></div>
                    </li>
                    <li>
                        <h2><span>Finalized</span></h2>
                        <div></div>
                    </li>
                    <li>
                        <h2><span>Completed</span></h2>
                        <div id="completed" class="quizzes">
                          <div class="quiz well">
                            <h6 class="for-dash"><%= link_to q.name, quiz_path(q) %></h6>
                            <%= link_to 'Delete', q, method: :delete, class: "span1.5 quiz-link btn btn-primary", data: { confirm: 'Are you sure?' } %>
                            <a class="span1.5 quiz-link btn btn-primary" href="<%=results_quiz_path(q)%>">Results</a>
                          </div>
                        </div>
                     </li>
                </ol>
                <noscript><p>Please enable JavaScript to get the full experience.</p></noscript>
          </div>
          <script>
              $(function() {
                $('#container').liteAccordion();
                });
            </script>

    <% end %>
